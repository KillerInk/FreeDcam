<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="UiSettingsChild">
        <attr name="HeaderText" format="string"/>
        <attr name="ValueText" format="string"/>
    </declare-styleable>
    <declare-styleable name="SettingsChildMenu">
        <attr name="Description" format="string"/>
    </declare-styleable>
    <declare-styleable name="ManualButton">
        <attr name="Header" format="string"/>
        <attr name="Image" format="integer"/>
    </declare-styleable>
    <declare-styleable name="MyGallery">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="NextGenTextItem">
        <attr name="setHeaderToView" format="string"/>
        <attr name="setValueToView" format="string"/>
    </declare-styleable>

    <declare-styleable name="NextGenSettingItem">
        <attr name="setDescriptionTextToView" format="string"/>
    </declare-styleable>

    <declare-styleable name="NextGenSettingBoolItem">
        <attr name="setBoolToView" format="boolean"/>
    </declare-styleable>
    
    <declare-styleable name="StrokedTextView">
        <attr name="setStrokeColor" format="color"/>
        <attr name="setStrokeSize" format="integer"/>
    </declare-styleable>
</resources>
